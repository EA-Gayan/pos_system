{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "crons": [
    {
      "path": "./services/expenseCleanUpService",
      "schedule": "0 0 * * *"
    },
    {
      "path": "./services/orderCleanupService",
      "schedule": "0 0 * * *"
    }
  ]
}
